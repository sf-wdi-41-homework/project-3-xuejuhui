    <h1>Welcome, <%= @user.first_name %>!</h1>
     <h1><%= @user.last_name %>!</h1>
      <h1><%= @user.email %>!</h1>
       <h1><%= @user.username%>!</h1>

 	<h1>
 		<% @recipes.each do |recipe|%>
	 		<p><%= recipe.name%></p>
	 		<p><%= recipe.date.to_date%></p>
	 		<p><% ingredient = recipe.ingredient%></p>
	 		<p><% ingredient.each do |ing|%></p>
	 		<%= ing %>
	 		<% end %>


 		<% end %>
 	</h1>
 	<h1>
 		<% @restaurants.each do |restuarant|%>
	 		<p><%=link_to restuarant.name, restuarant.link%></p>
	 		<p><%= restuarant.address%></p>
	 		<p><%= restuarant.price%></p>
	 		<p><%= restuarant.category%></p>
	 		<p><%= restuarant.created_at.to_date%></p>
 		<% end %>
 	</h1>